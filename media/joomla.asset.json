{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "{{name}}",
  "version": "{{version}}",
	"versionSub": "{{versionSub}}",
  "description": "{{description}}",
  "license": "{{licenseLong}}",
	"assets": [
		{
			"name": "{{nameReal}}.toTop",
			"type": "preset",
			"dependencies": [
				"{{nameReal}}.toTop#script",
				"{{nameReal}}.toTop#style"
			]
		},
		{
			"name": "{{nameReal}}.toTop",
			"type": "script",
			"version": "{{version}}",
			"uri": "{{nameReal}}/toTop.js",
			"attributes": {
				"defer": true
			}
		},
		{
			"name": "{{nameReal}}.toTop",
			"type": "style",
			"version": "{{version}}",
			"uri": "{{nameReal}}/toTop.css",
			"attributes": {
				"defer": true
			}
		},
		{
			"name": "{{nameReal}}.templatejs",
			"type": "script",
			"version": "{{version}}",
			"uri": "{{nameReal}}/template.js",
			"dependencies": [
				"jquery"
			]
		},
		{
			"name": "{{nameReal}}.bootstrap.es5.fallback.joomla5",
			"description": "Notfall-Asset",
			"type": "script",
			"deprecated": true,
			"uri": "",
			"dependencies": [
				"core"
			],
			"attributes": {
			"nomodule": true,
			"defer": true
			}
		}
	]
}
